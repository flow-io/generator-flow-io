{
  "name": "generator-flow-io",
  "version": "0.2.4",
  "description": "A Yeoman generator for flow.io modules.",
  "author": {
    "name": "Athan Reines",
    "email": "athan@nodeprime.com",
    "url": "https://github.com/kgryte"
  },
  "contributors": [
    {}
  ],
  "scripts": {
    "test": "./node_modules/.bin/mocha",
    "test-cov": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coverage -- -R spec",
    "coveralls": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --dir ./reports/coveralls/coverage --report lcovonly -- -R spec && cat ./reports/coveralls/coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./reports/coveralls"
  },
  "main": "app/index.js",
  "files": [
    "app"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/flow-io/generator-flow-io.git"
  },
  "keywords": [
    "yeoman-generator",
    "flow.io",
    "flow"
  ],
  "bugs": {
    "url": "https://github.com/flow-io/generator-flow-io/issues"
  },
  "dependencies": {
    "chalk": "^0.5.1",
    "npm-name": "^1.0.1",
    "shelljs": "^0.3.0",
    "yeoman-generator": "^0.17.3",
    "yosay": "^1.0.0"
  },
  "devDependencies": {
    "mocha": "1.x.x",
    "chai": "1.x.x",
    "coveralls": "^2.11.1",
    "istanbul": "^0.3.0"
  },
  "peerDependencies": {
    "yo": ">=1.0.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "http://www.opensource.org/licenses/MIT"
    }
  ]
}
